Dim PI = 3.1415926535897931
Dim E = 2.7182818284590451

Func Abs(x)
	if x < 0 then
        return -x
    else
        return x
    endif
EndFunc

Func Max(x, y)
    if x == y then
        return x
    endif

    if x > y then
        return x
    else
        return y
    endif
EndFunc

Func Min(x, y)
    if x == y then
        return x
    endif

    if x < y then
        return x
    else
        return y
    endif
EndFunc
